<service>
  <id>myservice</id>
  <name>My Service</name>
  <description>Runs a Java JAR as a Windows Service using WinSW</description>
  
  <executable>java</executable>
  <arguments>-jar myservice-1.0.0.jar</arguments>
  
  <env name="PATH" value="C:\Program Files\Java\jdk1.8.0_202\bin;%PATH%" />

  <logpath>logs</logpath>
  <log mode="roll" />

</service>